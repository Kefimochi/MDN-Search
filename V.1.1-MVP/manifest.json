{
  "name": "MDN Search Extension",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "html": ["search-window.html"]
    }
  ],
  "browser_action": {
    "default_popup": "search-window.html"
  }
}
